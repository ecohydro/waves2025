{
  "name": "research-lab-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:unit": "jest",
    "test:vitest": "vitest run",
    "test:vitest:ui": "vitest --ui",
    "test:fast": "vitest run --exclude=**/integration/**",
    "test:fast:ui": "vitest --ui --exclude=**/integration/**",
    "test:integration": "vitest run --config vitest.integration.config.ts tests/integration/cms-integration.test.ts",
    "test:integration:ui": "vitest --ui --config vitest.integration.config.ts tests/integration/cms-integration.test.ts",
    "test:sanity-data": "vitest run --config vitest.integration.config.ts tests/integration/sanity-data-validation.test.ts",
    "test:sanity-data:ui": "vitest --ui --config vitest.integration.config.ts tests/integration/sanity-data-validation.test.ts",
    "test:full-integration": "vitest run --config vitest.integration.config.ts tests/integration/full-cms-integration.test.ts",
    "test:full-integration:ui": "vitest --ui --config vitest.integration.config.ts tests/integration/full-cms-integration.test.ts",
    "validate:redirects": "tsx src/lib/migration/validate-redirects.ts",
    "generate:redirects": "tsx src/lib/migration/generate-url-mappings.ts && tsx src/lib/migration/update-next-config.ts",
    "migrate:people": "tsx src/lib/migration/migrate-people.ts",
    "migrate:people:dry": "tsx src/lib/migration/migrate-people.ts --dry-run",
    "migrate:people:verbose": "tsx src/lib/migration/migrate-people.ts --verbose",
    "migrate:people:enhanced": "tsx src/lib/migration/migrate-people-enhanced.ts",
    "migrate:people:enhanced:dry": "tsx src/lib/migration/migrate-people-enhanced.ts --dry-run",
    "migrate:people:enhanced:verbose": "tsx src/lib/migration/migrate-people-enhanced.ts --verbose",
    "migrate:publications": "tsx src/lib/migration/migrate-publications.ts",
    "migrate:publications:dry": "tsx src/lib/migration/migrate-publications.ts --dry-run",
    "migrate:publications:verbose": "tsx src/lib/migration/migrate-publications.ts --verbose",
    "migrate:publications:enhanced": "tsx src/lib/migration/migrate-publications-enhanced.ts",
    "migrate:publications:enhanced:dry": "tsx src/lib/migration/migrate-publications-enhanced.ts --dry-run",
    "migrate:publications:enhanced:verbose": "tsx src/lib/migration/migrate-publications-enhanced.ts --verbose",
    "migrate:news": "tsx src/lib/migration/migrate-news.ts",
    "migrate:news:dry": "tsx src/lib/migration/migrate-news.ts --dry-run",
    "migrate:news:verbose": "tsx src/lib/migration/migrate-news.ts --verbose",
    "enhance:pdfs": "tsx src/lib/migration/enhance-publications-with-pdfs.ts",
    "migrate:to-sanity:people": "tsx src/lib/cms/migration/migrate-people-to-sanity.ts",
    "migrate:to-sanity:people:dry": "tsx src/lib/cms/migration/migrate-people-to-sanity.ts --dry-run",
    "migrate:to-sanity:publications": "tsx src/lib/cms/migration/migrate-publications-to-sanity.ts",
    "migrate:to-sanity:publications:dry": "tsx src/lib/cms/migration/migrate-publications-to-sanity.ts --dry-run",
    "migrate:to-sanity:news": "tsx src/lib/cms/migration/migrate-news-to-sanity.ts",
    "migrate:to-sanity:news:dry": "tsx src/lib/cms/migration/migrate-news-to-sanity.ts --dry-run",
    "migrate:to-sanity:all": "tsx src/lib/cms/migration/index.ts",
    "migrate:to-sanity:all:dry": "tsx src/lib/cms/migration/index.ts --dry-run",
    "validate:sanity-migration": "tsx src/lib/cms/migration/validate-sanity-migration.ts",
    "sanity:dev": "sanity dev --port 3333",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "sanity:start": "sanity start",
    "studio": "npm run sanity:dev",
    "backup:create": "tsx src/lib/cms/migration/backup-system.ts",
    "backup:create:full": "tsx src/lib/cms/migration/backup-system.ts --include-images --validate",
    "backup:list": "tsx -e \"import { BackupSystem } from './src/lib/cms/migration/backup-system'; const bs = new BackupSystem(); bs.listBackups().then(b => console.log(JSON.stringify(b, null, 2)))\"",
    "backup:validate": "tsx -e \"import { BackupSystem } from './src/lib/cms/migration/backup-system'; const bs = new BackupSystem(); const dir = process.argv[1]; if (!dir) { console.error('Usage: npm run backup:validate <backup-dir>'); process.exit(1); } bs.validateBackup(dir).then(r => console.log(JSON.stringify(r, null, 2)))\"",
    "backup:restore": "tsx -e \"import { BackupSystem } from './src/lib/cms/migration/backup-system'; const bs = new BackupSystem(); const dir = process.argv[1]; const dryRun = process.argv[2] === '--dry-run'; if (!dir) { console.error('Usage: npm run backup:restore <backup-dir> [--dry-run]'); process.exit(1); } bs.restoreFromBackup(dir, { dryRun }).then(() => console.log('Restore completed'))\"",
    "test:api": "node scripts/test-api-endpoints.js",
    "test:api:dev": "BASE_URL=http://localhost:3000 node scripts/test-api-endpoints.js"
  },
  "dependencies": {
    "@sanity/client": "^7.6.0",
    "@sanity/code-input": "^5.1.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-url-secret": "^2.1.11",
    "@sanity/vision": "^3.95.0",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.0.0",
    "framer-motion": "^12.23.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.3",
    "next-auth": "^4.24.11",
    "next-sanity": "^9.12.0",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^19.1.0",
    "sanity-plugin-media": "^3.0.3",
    "sanity-plugin-mux-input": "^2.8.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@sanity/cli": "^3.95.0",
    "@sanity/types": "^3.95.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "sanity": "^3.95.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
